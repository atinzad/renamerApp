# Local config values used to prefill the UI (copy to .env)
# set -a && source .env && set +a
OAUTH_CLIENT_ID="YOUR_OAUTH_CLIENT_ID"
OAUTH_CLIENT_SECRET="YOUR_OAUTH_CLIENT_SECRET"
FOLDER_ID="YOUR_FOLDER_ID" #Optional
GOOGLE_DRIVE_ACCESS_TOKEN="YOUR_GOOGLE_DRIVE_ACCESS_TOKEN" #Optional (needs to be refreshed periodically)
OPENAI_API_KEY="YOUR_OPENAI_API_KEY" #Optional
EMBEDDINGS_PROVIDER="openai" # Switch: openai | local | sentence-transformers | bge-m3 | dummy
EMBEDDINGS_MODEL="text-embedding-3-large" # OpenAI embeddings model (when openai)
EMBEDDINGS_LOCAL_MODEL="BAAI/bge-m3" # Local sentence-transformers model (when local)
EMBEDDINGS_DEVICE="cpu" # Device for local embeddings: cpu | cuda
